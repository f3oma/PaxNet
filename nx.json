{
  "installation": {
    "version": "20.3.1"
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "typecheck": {
      "cache": true
    },
    "test": {
      "cache": true
    }
  },
  "useInferencePlugins": false,
  "defaultBase": "main",
  "generators": {
    "@nx/angular:component": {
      "style": "scss"
    }
  }
}
